
<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>



<table>
    @foreach (Dictionary<string, string> job in ViewBag.jobs)
    {

        @foreach (KeyValuePair<string, string> row in job)
        {

            <tr class="job-listing">
                <td>@row.Key</td>
                <td>@row.Value</td>
            </tr>

        }
        <tr><td colspan="2"></td></tr>
    }
</table>